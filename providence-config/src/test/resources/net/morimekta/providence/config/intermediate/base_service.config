def {
    http_port = 8080
    admin_port = 8088
    token = b64(AAAAAAAAAAAAAAAAAAAA)
}

config.Service {
    admin {
        port = admin_port
        oauth_token_key = token
    }
    http = {
        port = http_port
        context = "/app"

        signature_keys = {
            "app1": token
        }
        signature_override_keys = [
            "not_really_app_1"
        ]
    }
}
